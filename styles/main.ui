border: 2px dashed rgb(29, 35, 50);
}

#ajouterImageButton{
background-color: rgb(203, 203, 203)
}

#ajouterImageButton:hover{
background-color: rgb(190, 190, 190)
}

/* Table Container */
#tableOuterWidget{
